<% if(typeof success != 'undefined'){ %> <% success.forEach(function(succ) { %>
    <div class="row" id="alert_box">
        <div class="col s12 m12">
          <div class="card alertsuccess darken-1">
            <div class="row">
              <div class="col s12">
                <div class="card-content white-text messagestyle">
                  <p><%= succ.msg %></p>
              </div>
            </div>
            <div class="col s12 m2">
              <i class="fa fa-times icon_style" id="alert_close" aria-hidden="true"></i>
            </div>
          </div>
         </div>
        </div>
      </div>
      <% }); %>
      <% } %>
      <% if(typeof lerrors != 'undefined'){ %> <% lerrors.forEach(function(lerror) { %>
        <div class="row" id="alert_box">
            <div class="col s12 m12">
              <div class="card alerterror darken-1">
                <div class="row">
                  <div class="col s12">
                    <div class="card-content white-text messagestyle">
                      <p><%= lerror.msg%></p>
                  </div>
                </div>
                <div class="col s12 m2">
                  <i class="fa fa-times icon_style" id="alert_close" aria-hidden="true"></i>
                </div>
              </div>
             </div>
            </div>
          </div>
          <% }); %>
          <% } %>
          <% if(error != ''){ %>
            <div class="row" id="alert_box">
                <div class="col s12 m12">
                  <div class="card alerterror darken-1">
                    <div class="row">
                      <div class="col s12">
                        <div class="card-content white-text messagestyle">
                          <p><%= error %></p>
                      </div>
                    </div>
                    <div class="col s12 m2">
                      <i class="fa fa-times icon_style" id="alert_close" aria-hidden="true"></i>
                    </div>
                  </div>
                 </div>
                </div>
              </div>
              <% } %>
              <% if(succes != ''){ %>
                <div class="row" id="alert_box">
                    <div class="col s12 m12">
                      <div class="card alertsuccess darken-1">
                        <div class="row">
                          <div class="col s12">
                            <div class="card-content white-text messagestyle">
                              <p><%= succes %></p>
                          </div>
                        </div>
                        <div class="col s12 m2">
                          <i class="fa fa-times icon_style" id="alert_close" aria-hidden="true"></i>
                        </div>
                      </div>
                     </div>
                    </div>
                  </div>
                  <% } %>
    